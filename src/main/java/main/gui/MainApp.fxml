<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import org.kordamp.ikonli.javafx.*?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="main.gui.MainAppController">
   <children>
      <VBox prefHeight="400.0" prefWidth="600.0" spacing="30.0" AnchorPane.bottomAnchor="20.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="20.0">
         <children>
            <VBox prefWidth="560.0" spacing="20.0">
               <children>
                  <HBox alignment="CENTER_LEFT" spacing="100.0">
                     <children>
                        <HBox alignment="CENTER" spacing="30.0">
                           <children>
                              <Label text="Code fournisseur" />
                              <TextField fx:id="inputCodeFournis" prefHeight="25.0" prefWidth="200.0" promptText="Saisir le code du fournisseur..." />
                           </children>
                        </HBox>
                        <Button fx:id="BtnRecherche" mnemonicParsing="false" onAction="#btnRecherche" prefHeight="25.0" prefWidth="141.0" text="Recherche" />
                     </children>
                  </HBox>
                  <HBox>
                     <children>
<!--                        <FontIcon fx:id="icon" iconColor="#a2d074" iconLiteral="fa-close" visible="false"/>-->
                        <Label fx:id="message" disable="true" prefHeight="44.0" prefWidth="560.0" text="Label" visible="false" />
                     </children>
                  </HBox>
               </children>
            </VBox>
            <VBox alignment="CENTER" prefHeight="251.0" prefWidth="560.0" spacing="40.0" style="-fx-border-color: black; -fx-border-width: 0.5pt;">
               <children>
                  <GridPane hgap="20.0">
                     <columnConstraints>
                        <ColumnConstraints fillWidth="false" hgrow="SOMETIMES" maxWidth="164.0" minWidth="10.0" prefWidth="80.0" />
                        <ColumnConstraints hgrow="SOMETIMES" maxWidth="290.0" minWidth="10.0" prefWidth="200.0" />
                        <ColumnConstraints hgrow="SOMETIMES" maxWidth="261.0" minWidth="10.0" prefWidth="200.0" />
                     </columnConstraints>
                     <rowConstraints>
                        <RowConstraints minHeight="10.0" vgrow="SOMETIMES" />
                        <RowConstraints minHeight="10.0" vgrow="SOMETIMES" />
                        <RowConstraints minHeight="10.0" vgrow="SOMETIMES" />
                        <RowConstraints minHeight="10.0" vgrow="SOMETIMES" />
                        <RowConstraints minHeight="10.0" vgrow="SOMETIMES" />
                        <RowConstraints minHeight="10.0" vgrow="SOMETIMES" />
                        <RowConstraints minHeight="10.0" vgrow="SOMETIMES" />
                        <RowConstraints minHeight="10.0" vgrow="SOMETIMES" />
                        <RowConstraints minHeight="10.0" vgrow="SOMETIMES" />
                     </rowConstraints>
                     <children>
                        <Label text="Nom" />
                        <TextField fx:id="inputNom" editable="false" GridPane.columnIndex="1" />
                        <Label text="Adresse" GridPane.rowIndex="2" />
                        <TextField fx:id="inputAdresse" editable="false" GridPane.columnIndex="1" GridPane.rowIndex="2" />
                        <Label text="Ville / CP" GridPane.rowIndex="4" />
                        <TextField fx:id="inputVille" editable="false" GridPane.columnIndex="1" GridPane.rowIndex="4" />
                        <TextField fx:id="inputCP" editable="false" GridPane.columnIndex="2" GridPane.rowIndex="4" />
                        <Label text="Contact" GridPane.rowIndex="7" />
                        <TextField fx:id="inputContact" editable="false" GridPane.columnIndex="1" GridPane.rowIndex="7" />
                     </children>
                  </GridPane>
               </children>
               <VBox.margin>
                  <Insets />
               </VBox.margin>
               <padding>
                  <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
               </padding>
            </VBox>
         </children>
         <padding>
            <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
         </padding>
      </VBox>
   </children>
